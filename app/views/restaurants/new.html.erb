<%= form_for @restaurant do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :category %>
  <%= form.text_field :category %>
  <%= form.label :address %>
  <%= form.text_field :address %>
  <%= form.label :phone_number %>
  <%= form.text_field :phone_number %>
  <%= form.submit %>
<% end %>
